# CFEngine
[ -d /var/cfengine/bin ] && PATH=$(_pathedit $PATH /var/cfengine/bin after)

# Dell server admin tools
[ -d /opt/dell/srvadmin/sbin ] && PATH=$(_pathedit $PATH /opt/dell/srvadmin/sbin after)
[ -d /opt/dell/srvadmin/bin ] && PATH=$(_pathedit $PATH /opt/dell/srvadmin/bin after)

# Couchbase
[ -d /opt/couchbase/bin ] && PATH=$(_pathedit $PATH /opt/couchbase/bin after)

# Shim for console to change TITLE
_have console && {
# Set console path env variable
[ -z "$CONSOLE_PATH" ] && readonly CONSOLE_PATH=$(command -v console)
console_shim()
{
	local params exitstatus
	params=$@
	# Change title
	_set_title $params
	# Execute console
	if [ -n "$CONSOLE_PATH" ] && [ -x "$CONSOLE_PATH" ] ; then
		$CONSOLE_PATH $params
		exitstatus=$?
	fi
	return $exitstatus
}
alias console='console_shim'
}
